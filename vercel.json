{
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/submit_registration",
      "dest": "/app.py"
    },
    {
      "src": "/game",
      "dest": "/app.py"
    },
    {
      "src": "/start_game",
      "dest": "/app.py"
    }
  ],
  "env": {
    "FLASK_APP": "app.py",
    "FLASK_ENV": "production"
  }
}
